<% if logged_in? %>
  <div class="row">
    <div class="col-sm-8">
      <%= render 'cmposts/cmposts', cmposts: @cmposts %>
    </div>
  </div>
<% else %>
<div class="center jumbotron">
  <div class="text-center">
    <h1>Welcome to the CMposts</h1>
    <%= link_to 'Sign up now!', signup_path, class: 'btn btn-lg btn-primary' %>
  </div>
</div>
<% end %>